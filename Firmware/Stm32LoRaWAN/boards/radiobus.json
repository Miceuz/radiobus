{
  "build": {
    "core": "stm32",
    "cpu": "cortex-m4",
    "extra_flags": "-DSTM32WLxx -DSTM32WLE5xx",
    "framework_extra_flags": {
      "arduino": "-DUSE_CM4_STARTUP_FILE"
    },
    "mcu": "stm32wle5cc",
    "product_line": "STM32WLE5xx",
    "variant": "STM32WLxx/WL54CCU_WL55CCU_WLE4C(8-B-C)U_WLE5C(8-B-C)U",
    "arduino": {
      "board": "GENERIC_WLE5CCUX",
      "variant_h": "variant_generic.h"
    },
    "cmsis_lib_gcc": "arm_cortexM4lf_math"
  },
  "debug": {
    "default_tools": [
      "stlink"
    ],
    "jlink_device": "STM32WL55JC",
    "onboard_tools": [
      "stlink"
    ],
    "openocd_target": "stm32wlx",
    "svd_path": "STM32WL5x_CM4.svd"
  },
  "frameworks": [
    "arduino",
    "cmsis",
    "stm32cube"
  ],
  "name": "Catnip Radiobus V3",
  "upload": {
    "maximum_ram_size": 65536,
    "maximum_size": 262144,
    "protocol": "serial",
    "protocols": [
      "dfu",
      "serial",
      "jlink",
      "stlink",
      "blackmagic"
    ],
    "gpiostring": "-dtr,-rts,"
  },
  "url": "https://www.catnip.lt",
  "vendor": "Catnip Electronics"
}